
<div class="container">
  <div class="row">
    <div class="col-sm-8">
      <div class="row">
        
      </div>
    </div>
    <div class="col-sm-4">
      @foreach (var comment in Model)
      {
        <div class="row">
          <h3 class="col-sm-3">@comment.KheechEvent.ApplicationUser.FirstName</h3>
          <p class="col-sm-9"> @comment.KheechEvent.ApplicationUser.FirstName added: <br>
            <small> @comment.Discussion </small>
          </p>
        </div>
      }
    </div>
  </div>
</div>
